{
  "name": "@danteissaias/ds",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    }
  },
  "scripts": {
    "dev": "vite serve dev",
    "vercel-build": "vite build dev",
    "build": "BUILD_LIB=true vite build && dts-bundle-generator -o dist/index.d.ts src/index.ts"
  },
  "dependencies": {
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "dts-bundle-generator": "^7.2.0",
    "lightningcss": "^1.19.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "typescript": "^4.9.4",
    "vite": "^4.1.1",
    "vite-plugin-lightningcss": "^0.0.5",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10"
  }
}
